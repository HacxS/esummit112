        <div class="content mypart" style="min-height:auto;">
            <div class="card" style="padding-bottom: 50px;color: white;">
                <div class="card-header card-header-primary">
                    <h4 class="card-title">Registered events details</h4>
                    <p class="card-category">Note: If you are the leader of the team, then deleting/leaving the team would dissolve the entire team. </p>
                </div>
                <div class="container-fluid">
                    <% registeredEvents.forEach(t => { %>
                        <div class="row" style="    margin-top: 30px;font-size: 24px;text-align: center;">
                            <div class="col-6">
                                    
                                    <small style="font-size: 12px;text-decoration: underline;color:#8b92a9 ;">Event Name</small>
                                    <p style="margin: 0;    margin-top: 3px;"><%= t.name %></p>
                            </div>
                            <div class="col-6 ">
                                    
                                    <small style="font-size: 12px;text-decoration: underline;color: #8b92a9;">Team Name</small>
                                    <p style="margin: 0;    margin-top: 3px;"><%= t.team_name %></p> 
                            </div>
                        </div>
        
                        <br>
                        <p style="font-size: 25px;  margin: 0;margin-top: 15px;  text-align: center;color: #8b92a9;  text-decoration: underline;">Team</p>
                        <div class="card-body">
                            <div class="table-responsive">
                                <table class="table">
                                    <thead class=" text-primary">
                                        <th>Email ID</th>
                                        <th>Position</th>
                                        <th>Payment/Registration</th>
                                        <th>Action</th>
                                    </thead>
                                    <tbody>
                                    <% allteam.forEach(o => { %>
                                        <% if(o.team_name == t.team_name ){ %>
                                            <% o.data.forEach(p => { %>
                                              <% if(p.name == t.name){ %>
                                            <tr>
                                                <td>
                                                    <% if(p.student_id == user.email){ %>
                                                        <i style="position: relative;top: 6px;" class="material-icons">persons</i>
                                                        <% } %>
                                                    <%= p.student_id %></td>
                                            <% if(p.leader_id == p.student_id){ %>
                                                <td>Leader</td>
                                            <% }else{ %>
                                                <td>Participant</td>
                                            <% } %>
                                            <% if( p.payment == true){ %>
                                              <td>Done</td>
                                              <% }else{ %>
                                                <td>Pending</td>
                                                <% } %>
                                              <td>
                                            <% if(p.student_id == user.email && p.status==false){ %>
                                                <td class="text-primary">
                                                    <form id="add-member" method="get" action="/dashboard/accept-event/<%= t._id %>">
                                                        <button type="submit" style="background-color: #70ae26;" class="btn btn-outline-success"> Accpet </button>
                                                    </form>
                                                </td>
                                            <% } %>
                                            <% if((p.leader_id != user.email && p.student_id== user.email)  ||  (p.student_id != p.leader_id && p.student_id == user.email) || (p.leader_id == user.email && p.student_id != user.email)){ %>
                                                <td>
                                                    <form id="add-member" method="get"  action="/dashboard/reject-event/<%= p._id %>">
                                                        <button style="background-color: #d52c2c;" type="submit"  class="deleteItem btn btn-outline-danger"> Delete </button>
                                                    </form>
                                                </td>
                                            <% } %>
                                            </tr>
                                            <% } %>
                                            <% }) %>
                                            <!-- <p> <%= o.data.length %></p> -->
                                        <% } %>
                                    <% }) %> 
                                    </tbody>
                                </table>
                            </div>
                        </div> 

                        <% if(user.email == t.leader_id){ %>
                        <form id="add-member" method="post" action="/dashboard/add-member-event/<%= t.event_id %>/<%= t.team_name%>/<%= t.name %>">
                            <div class="form-group add-mem">
                                <label for="exampleInputEmail1">Add  Member</label>
                                <input type="email" name="email" class="form-control" id="exampleInputEmail1" aria-describedby="emailHelp" required>
                                <small id="emailHelp" class="form-text text-muted">Enter email id of your friend.</small>
                                <button type="submit" class="btn btn-outline-light" >Add</button>
                            </div>
                        </form>
                        <% } %>

                        <% if( t.leader_id == user.email ){ %>
                            <form id="add-member" method="get" style="width: max-content; margin: 10px auto;" action="/dashboard/discard-event/<%= t.team_name %>/<%= t.name %>">
                                <button style="background-color: #d52c2c;"  type="submit"  class="deleteItem delete-event btn btn-outline-danger"> Delete Event </button>
                            </form>
                        <% } %>
                        <hr style="border: 1px solid #8026a1;">
                    <%  }) %>

                    <% if(registeredEvents.length == 0){ %>
                        <h3 style="text-align: center;font-size: 16px;">You have not added any events yet.</h3>
